OCAMLLOC=loc
if test "$OCAMLC" != ":" ; then
	{ echo "$as_me:$LINENO: checking for Ocaml header files" >&5
echo $ECHO_N "checking for Ocaml header files... $ECHO_C" >&6; }
	dirs="/usr/lib/ocaml/caml /usr/local/lib/ocaml/caml"
	dir="`$OCAMLC -where 2>/dev/null`"
	if test "$dir"; then
		dirs="$dir/caml $dirs"
	fi
	for i in $dirs; do
		if test -r $i/mlvalues.h; then
			{ echo "$as_me:$LINENO: result: $i" >&5
echo "${ECHO_T}$i" >&6; }
			OCAMLEXT="$i"
			OCAMLINC="-I$OCAMLEXT"
			break;
		fi
	done
	if test -z "$OCAMLINC"; then
		{ echo "$as_me:$LINENO: result: not found" >&5
echo "${ECHO_T}not found" >&6; }
	fi

	{ echo "$as_me:$LINENO: checking for Ocaml version 3.08.2 or higher" >&5
echo $ECHO_N "checking for Ocaml version 3.08.2 or higher... $ECHO_C" >&6; }
	OCAMLVER=`$OCAMLC -version | sed -e 's/.*version //g'`

	# Split the version into units.
	ver_a="$OCAMLVER"
	ver_b="3.08.2"
	nodots_a=`echo $ver_a | sed -e 's/\./ /g'`
	condition="equal"
	isolate_b_regex='\([0-9]\+\).*'
	for ver_part in $nodots_a ; do
		b_ver_part=`echo "$ver_b" | sed -e 's/'"$isolate_b_regex"'/\1/'`
		if test \( "$ver_part" -lt "$b_ver_part" \) -a \( "x$condition" == "xequal" \) ; then
			condition=less
		elif test \( "$ver_part" -gt "$b_ver_part" \) -a \( "x$condition" == "xequal" \) ; then
			condition=greater
		fi
		isolate_b_regex='[0-9]\+\.'"$isolate_b_regex"
	done

	if test "x$condition" == "xequal" ; then
		:
	elif test "x$condition" == "xless" ; then
		:
	elif test "x$condition" == "xgreater" ; then
		OCAMLLOC=_loc
	fi

	{ echo "$as_me:$LINENO: result: $OCAMLVER" >&5
echo "${ECHO_T}$OCAMLVER" >&6; }
fi
fi # Disabling ocaml

